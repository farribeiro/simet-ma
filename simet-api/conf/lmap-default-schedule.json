{"ietf-lmap-control:lmap":{
"agent":{"report-agent-id":true},
"schedules":{"schedule":[
  { "name":"ma-local_default-sched-refresh",
    "start":"every-six-hours",
    "execution-mode":"sequential",
    "action":[ {
      "name":"simet.nic.br_reschedule",
      "task":"simet.nic.br_reschedule"
      }
    ] },
  { "name":"ma-local_default-periodic-test-run",
    "start":"every-four-hours",
    "execution-mode":"sequential",
    "action":[ {
      "name":"simet.nic.br_basic-tests",
      "task":"simet.nic.br_basic-tests",
      "destination":[
	"report-primary"
      ] }
    ] },
  { "name":"report-primary",
    "start":"every-six-hours",
    "execution-mode":"sequential"
  }
]},"events":{"event":[
  { "name":"every-minute","periodic":{"interval":60} },
  { "name":"every-three-hours","random-spread":3600000,
    "calendar":{ "month":["*"],"day-of-month":["*"],"day-of-week":["*"],
      "hour":[0,3,6,9,12,15,18,21],"minute":[0],"second":[0] } },
  { "name":"every-four-hours","random-spread":3600000,
    "calendar":{ "month":["*"],"day-of-month":["*"],"day-of-week":["*"],
      "hour":[0,4,8,16,20],"minute":[0],"second":[0] } },
  { "name":"every-six-hours","random-spread":3600000,
    "calendar":{ "month":["*"],"day-of-month":["*"],"day-of-week":["*"],
      "hour":[0,6,12,18],"minute":[0],"second":[0] } },
  {"name":"daily",
    "calendar":{"month":["*"],"day-of-month":["*"],"day-of-week":["*"],
      "hour":[0],"minute":[0],"second":[0],"timezone-offset":"+00:00" }
  }
]} }}
